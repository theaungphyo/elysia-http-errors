		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"@types/bun": "latest",
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"elysia": "^1.4.22"
	},
	"peerDependencies": {
		"typescript": "^5"
	}
}
