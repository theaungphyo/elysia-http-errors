{
	"name": "elysia-plugins",
	"version": "1.0.0",
	"description": "Production-ready Elysia.js plugins for Rate Limiting, Logging, and Error Handling.",
	"main": "src/index.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "bun run --watch src/index.ts",
